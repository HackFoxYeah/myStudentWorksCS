        <Label Content="a" Margin="47,46,201,155" FontWeight="Bold" FontSize="16"/>
        <Label Content="b" Margin="47,93,198,108" FontWeight="Bold" FontSize="16"/>
        <Label Content="S" Margin="58,49,179,0" VerticalAlignment="Top" FontWeight="Bold" Height="34" FontSize="16" FontFamily="Trebuchet MS" Grid.Column="1"/>
        <Label Content="P" Margin="58,94,179,0" VerticalAlignment="Top" FontWeight="Bold" Height="34" FontSize="16" FontFamily="Trebuchet MS" Grid.Column="1"/>
        <TextBox x:Name="fifthTextBox" Margin="70,165,50,29" TextWrapping="Wrap" MaxLength="9" TextChanged="FifthTextBox_TextChanged" BorderBrush="Black" FontSize="27"/>
        <Label Content="xyz" Margin="30,167,198,34" FontWeight="Bold" FontSize="16"/>
        <TextBox x:Name="sixthTextBox" MaxLength="5" IsReadOnly="True" Margin="227,165,207,29" TextWrapping="Wrap" TextDecorations="{x:Null}" BorderBrush="Black" FontSize="27" Grid.ColumnSpan="2"/>
        <Label Content="zyx" Margin="57,167,164,34" FontWeight="Bold" FontSize="16" Grid.Column="1"/>
        <Button x:Name="buttonAbout" Content="About" Margin="127,179,56,26" FontSize="18" FontStyle="Normal" FontWeight="Normal" FontFamily="Yu Gothic UI Semilight" Click="About_Click" BorderBrush="White" Background="#FF7CFFE4" Grid.Column="1"/>
