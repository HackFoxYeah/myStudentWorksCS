        <Button x:Name="buttonCancel2" Content="Canc. 2" Margin="176,79,21,126" FontSize="18" FontStyle="Normal" FontWeight="Normal" FontFamily="Yu Gothic UI Semilight" Click="To_Default2_Click" BorderBrush="White" Background="#FFFFE089" Grid.Column="1"/>
        <GroupBox Header="1" Grid.ColumnSpan="2" Margin="39,22,174,95"/>
        <GroupBox Header="2" Grid.ColumnSpan="2" Margin="25,147,163,21" RenderTransformOrigin="0.5,0.5">
            <GroupBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="1.273"/>
                    <RotateTransform/>
                    <TranslateTransform X="0.644"/>
                </TransformGroup>
            </GroupBox.RenderTransform>
        </GroupBox>
    </Grid>
</Window>
