        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="38*"/>
            <ColumnDefinition Width="37*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="buttonCancel1" Content="Canc. 1" Margin="98,79,99,126" FontSize="18" FontStyle="Normal" FontWeight="Normal" FontFamily="Yu Gothic UI Semilight" Click="To_Default1_Click" BorderBrush="White" Background="#FFFFE089" Grid.Column="1"/>
        <TextBox x:Name="firstTextBox" Margin="70,42,50,152" TextWrapping="Wrap" MaxLength="9" Background="White" BorderBrush="Black" TextChanged="FirstTextBox_TextChanged" FontSize="27"/>
        <TextBox x:Name="thirdTextBox" IsReadOnly="True" Margin="228,42,206,152" MaxLength="5" TextWrapping="Wrap" TextDecorations="{x:Null}" BorderBrush="Black" FontSize="27" Grid.ColumnSpan="2"/>
        <TextBox x:Name="secondTextBox" Margin="70,89,50,105" TextWrapping="Wrap" MaxLength="9" TextChanged="SecondTextBox_TextChanged" BorderBrush="Black" FontSize="27"/>
        <TextBox x:Name="fourthTextBox" MaxLength="5" IsReadOnly="True" Margin="228,89,206,105" TextWrapping="Wrap" TextDecorations="{x:Null}" BorderBrush="Black" FontSize="27" Grid.ColumnSpan="2"/>
